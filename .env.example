# Server
ORCH_SERVER_PORT=3000

# Jira
ORCH_JIRA_BASE_URL=https://jira.twiket.com
ORCH_JIRA_TOKEN=your-jira-token

# Bitbucket
ORCH_BITBUCKET_BASE_URL=https://bitbucket.twiket.com
ORCH_BITBUCKET_TOKEN=your-bitbucket-token

# Confluence (optional)
ORCH_CONFLUENCE_BASE_URL=https://confluence.twiket.com
ORCH_CONFLUENCE_TOKEN=your-confluence-token

# Loop/Mattermost (optional)
ORCH_LOOP_BASE_URL=https://onetwotrip.loop.ru
ORCH_LOOP_TOKEN=your-loop-token

# Notifier (OpenClaw webhook)
ORCH_NOTIFIER_WEBHOOK_URL=http://localhost:4000/webhook
ORCH_NOTIFIER_HMAC_SECRET=your-hmac-secret

# Agent
ORCH_AGENT_MODEL=o3
ORCH_AUTH_DIR=~/.codex

# SSH key for git
SSH_KEY_PATH=~/.ssh
